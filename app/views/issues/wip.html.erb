<div class="content-full">
  <h1>WIP</h1>
  
  <script>
  $(function () { 
      $('#container').highcharts({
          chart: {
              type: 'bar'
          },
          title: {
              text: 'Fruit Consumption'
          },
          xAxis: {
              categories: ['Apples', 'Bananas', 'Oranges']
          },
          yAxis: {
              title: {
                  text: 'Fruit eaten'
              }
          },
          series: [{
              name: 'Jane',
              data: [1, 0, 4]
          }, {
              name: 'John',
              data: [5, 7, 3]
          }]
      });
  });
  </script>
  
  <div id="container" style="width:100%; height:400px;"></div>
  
  <table>
    <% @wip_histories.each do |history| %>
      <tr>
        <td><%= history.date %></td>
        <td><%= history.wip %></td>
      </tr>
    <% end %>
  </table>
</div>
